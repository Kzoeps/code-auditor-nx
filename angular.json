          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/feature/audit/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    },
    "team": {
      "projectType": "library",
      "root": "libs/feature/team",
      "sourceRoot": "libs/feature/team/src",
      "prefix": "selise-start",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature/team/tsconfig.lib.json",
              "libs/feature/team/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/feature/team/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/feature/team/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    },
    "auth": {
      "projectType": "library",
      "root": "libs/feature/auth",
      "sourceRoot": "libs/feature/auth/src",
      "prefix": "selise-start",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature/auth/tsconfig.lib.json",
              "libs/feature/auth/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/feature/auth/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/feature/auth/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    },
    "shared": {
      "projectType": "library",
      "root": "libs/feature/shared",
      "sourceRoot": "libs/feature/shared/src",
      "prefix": "selise-start",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature/shared/tsconfig.lib.json",
              "libs/feature/shared/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/feature/shared/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/feature/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "code-auditor"
}
